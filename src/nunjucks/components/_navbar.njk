{% macro navbar(links, className) %}
  <nav class='nav{{ ' ' + className if className }}'>
    <ul>
      {% for link in links %}
        <li class='nav__item'>
          <a class='nav__link' href='{{ link.href }}'>{{ link.label }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
{% endmacro %}
